{
  "$id": "https://github.com/data-fair/simple-directory/sites/patch-req",
  "x-exports": [
    "types",
    "validate"
  ],
  "title": "Patch site req",
  "type": "object",
  "required": ["body"],
  "properties": {
    "body": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "_id",
        "authMode"
      ],
      "properties": {
        "_id": {
          "readOnly": true,
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/_id"
        },
        "theme": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/theme"
        },
        "logo": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/logo"
        },
        "reducedPersonalInfoAtCreation": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/reducedPersonalInfoAtCreation"
        },
        "tosMessage": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/tosMessage"
        },
        "authMode": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/authMode"
        },
        "authOnlyOtherSite": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/authOnlyOtherSite"
        },
        "authProviders": {
          "$ref": "https://github.com/data-fair/simple-directory/site#/properties/authProviders"
        }
      }
    }
  }
}