{
  "name": "simple-directory",
  "version": "3.5.0",
  "description": "Easy users and organizations management for modern Web-oriented architectures.",
  "main": "server/app.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --exit --bail --timeout 20000 test/",
    "test-cover": "nyc npm test",
    "test-deps": "docker-compose --profile test up -d",
    "report": "nyc report --reporter=html",
    "lint": "eslint --ext js,vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext js,vue --ignore-path .gitignore .",
    "dev-server": "NODE_ENV=development nodemon server",
    "dev-client": "npm run link-doc && NODE_ENV=development nuxt",
    "dev-deps": "docker-compose --profile dev up -d",
    "build": "npm run link-doc && nuxt build",
    "start": "node server",
    "analyze": "nuxt build --analyze",
    "doc": "nuxt --config-file ${PWD}/doc/nuxt.config.js",
    "link-doc": "ln -nsf ${PWD}/doc/pages/doc public/pages/doc",
    "build-doc": "nuxt generate --config-file ${PWD}/doc/nuxt.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koumoul-dev/simple-directory.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "no-only-tests"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/no-v-html": "off",
      "vue/multi-word-component-names": "off",
      "node/no-deprecated-api": "off",
      "vue/no-mutating-props": "off",
      "vue/require-prop-types": "off",
      "vue/no-useless-template-attributes": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/koumoul-dev/simple-directory/issues"
  },
  "homepage": "https://github.com/koumoul-dev/simple-directory#readme",
  "devDependencies": {
    "@data-fair/sd-vue": "1.4.1-beta.8",
    "@koumoul/v-iframe": "^0.10.0",
    "@koumoul/vue-multianalytics": "^1.10.1",
    "@mdi/font": "^6.5.95",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "clean-modules": "^2.0.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.5.0",
    "iframe-resizer": "^4.0.1",
    "jwt-decode": "^2.2.0",
    "mocha": "^9.2.1",
    "nuxt": "^2.15.8",
    "nuxt-webpack-optimisations": "^2.2.0",
    "nyc": "^15.1.0",
    "push-dir": "^0.4.1",
    "semver-extract": "^1.1.1",
    "tinycolor2": "^1.4.2",
    "vue-cropperjs": "^4.1.0",
    "vue-moment": "^4.1.0"
  },
  "dependencies": {
    "@koumoul/nuxt-config-inject": "^0.4.10",
    "@koumoul/sd-express": "1.1.2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/markdownit": "^2.0.0",
    "accept-language-parser": "^1.5.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "cacheable-lookup": "^6.0.4",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.3",
    "cookie-universal-nuxt": "^2.1.5",
    "cookies": "^0.7.3",
    "dayjs": "^1.10.7",
    "debug": "^3.2.6",
    "email-validator": "^2.0.4",
    "event-to-promise": "^0.8.0",
    "express": "^4.17.1",
    "flat": "^4.1.0",
    "fs-extra": "^8.1.0",
    "json-web-key": "^0.3.0",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "ldapjs": "^2.2.3",
    "maildev": "^1.1.0",
    "material-colors": "^1.2.6",
    "mjml": "^4.8.1",
    "mongodb": "^3.6.4",
    "multer": "^1.4.2",
    "no-avatar": "^1.0.1",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.18.4",
    "nuxt-i18n": "^6.27.0",
    "nuxt-start": "^2.15.8",
    "original-url": "^1.2.3",
    "otplib": "^12.0.1",
    "prom-client": "^14.0.0",
    "qrcode": "^1.4.4",
    "rate-limiter-flexible": "^2.0.0",
    "request-ip": "^2.1.3",
    "seedrandom": "^3.0.5",
    "shortid": "^2.2.13",
    "simple-oauth2": "^3.4.0",
    "uuid": "^8.3.2"
  }
}
