{
  "name": "simple-directory",
  "version": "6.0.1",
  "description": "Easy users and organizations management for modern Web-oriented architectures.",
  "main": "server/app.js",
  "scripts": {
    "test": "npm run test-file && npm run test-mongo && npm run test-ldap",
    "test-file": "NODE_ENV=test STORAGE_TYPE=file mocha --exit --bail --timeout 20000 test/file/*.js",
    "test-mongo": "NODE_ENV=test STORAGE_TYPE=mongo mocha --exit --bail --timeout 20000 test/mongo",
    "test-ldap": "NODE_ENV=test STORAGE_TYPE=ldap mocha --exit --bail --timeout 20000 test/ldap",
    "test-cover": "nyc npm test",
    "test-deps": "docker compose --profile test up -d",
    "report": "nyc report --reporter=html",
    "lint": "eslint --ext js,vue .",
    "lint-fix": "eslint --fix --ext js,vue .",
    "dev-server": "NODE_ENV=development nodemon server",
    "dev-client": "npm run link-doc && NODE_ENV=development nuxt",
    "dev-deps": "docker compose --profile dev up -d",
    "build": "npm run link-doc && nuxt build --no-generate",
    "start": "node server",
    "analyze": "nuxt build --no-generate --analyze",
    "doc": "nuxt --config-file ${PWD}/doc/nuxt.config.js",
    "link-doc": "ln -nsf ${PWD}/doc/pages/doc public/pages/doc",
    "build-doc": "nuxt generate --config-file ${PWD}/doc/nuxt.config.js",
    "build-types": "cd types && npm run build",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koumoul-dev/simple-directory.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "no-only-tests"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "vue/no-v-html": "off",
      "vue/multi-word-component-names": "off",
      "node/no-deprecated-api": "off",
      "vue/no-mutating-props": "off",
      "vue/require-prop-types": "off",
      "vue/no-useless-template-attributes": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/koumoul-dev/simple-directory/issues"
  },
  "homepage": "https://github.com/koumoul-dev/simple-directory#readme",
  "devDependencies": {
    "@data-fair/sd-vue": "^2.6.1",
    "@koumoul/v-iframe": "^0.20.3",
    "@koumoul/vjsf": "^2.21.3",
    "@koumoul/vue-multianalytics": "^1.10.1",
    "@mdi/font": "^7.0.96",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "clean-modules": "^2.0.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.5.0",
    "iframe-resizer": "^4.0.1",
    "jwt-decode": "^3.1.2",
    "mocha": "^9.2.1",
    "nuxt": "^2.16.2",
    "nuxt-webpack-optimisations": "^2.2.0",
    "nyc": "^15.1.0",
    "patch-package": "^6.5.1",
    "push-dir": "^0.4.1",
    "semver-extract": "^1.1.1",
    "tinycolor2": "^1.4.2",
    "vue-cropperjs": "^4.1.0",
    "vue-moment": "^4.1.0"
  },
  "dependencies": {
    "@data-fair/sd-express": "^1.4.0",
    "@koumoul/nuxt-config-inject": "^0.4.10",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.3.1",
    "@nuxtjs/markdownit": "^2.0.0",
    "accept-language-parser": "^1.5.0",
    "axios": "^1.3.4",
    "body-parser": "^1.18.3",
    "cacheable-lookup": "^6.0.4",
    "capitalize": "^2.0.4",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.3",
    "cookie-universal-nuxt": "^2.1.5",
    "cookies": "^0.7.3",
    "csv-stringify": "^6.2.4",
    "dayjs": "^1.10.7",
    "debug": "^4.3.4",
    "email-validator": "^2.0.4",
    "escape-string-regexp": "^4.0.0",
    "event-to-promise": "^0.8.0",
    "express": "^4.17.1",
    "flat": "^4.1.0",
    "fs-extra": "^8.1.0",
    "gm": "^1.25.0",
    "http-errors": "^2.0.0",
    "http-terminator": "^3.2.0",
    "initials": "^3.1.2",
    "json-web-key": "^0.3.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^1.3.0",
    "ldapjs": "^2.2.3",
    "maildev": "^1.1.0",
    "material-colors": "^1.2.6",
    "memoizee": "^0.4.15",
    "mjml": "^4.8.1",
    "moment": "^2.29.4",
    "mongodb": "^4.13.0",
    "multer": "^1.4.2",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.18.4",
    "nuxt-start": "^2.16.2",
    "otplib": "^12.0.1",
    "prom-client": "^14.0.0",
    "qrcode": "^1.4.4",
    "rate-limiter-flexible": "^2.0.0",
    "request-ip": "^2.1.3",
    "samlify": "2.8.8",
    "seedrandom": "^3.0.5",
    "shortid": "^2.2.13",
    "simple-oauth2": "^3.4.0",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2",
    "vuex": "^3.6.2"
  }
}
