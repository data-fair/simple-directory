{
  "name": "simple-directory",
  "version": "3.0.0",
  "description": "Easy users and organizations management for modern Web-oriented architectures.",
  "main": "server/app.js",
  "scripts": {
    "test": "ava --timeout=10000 --fail-fast --serial -v test/*.js",
    "test-cover": "nyc ava --timeout=10000 --fail-fast --serial -v test/*.js",
    "test-single": "ava --timeout=10000 --serial -v",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report --reporter=html",
    "lint": "eslint --ext js,vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext js,vue --ignore-path .gitignore .",
    "dev-server": "NODE_ENV=development nodemon server",
    "dev-client": "npm run link-doc && NODE_ENV=development nuxt",
    "build": "npm run link-doc && nuxt build",
    "start": "node server",
    "analyze": "nuxt build --analyze",
    "doc": "nuxt --config-file ${PWD}/doc/nuxt.config.js",
    "link-doc": "ln -nsf ${PWD}/doc/pages/doc public/pages/doc",
    "build-doc": "nuxt generate --config-file ${PWD}/doc/nuxt.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koumoul-dev/simple-directory.git"
  },
  "author": "",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/koumoul-dev/simple-directory/issues"
  },
  "homepage": "https://github.com/koumoul-dev/simple-directory#readme",
  "devDependencies": {
    "ava": "^3.15.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.28.0",
    "eslint-config-vuetify": "^0.6.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "nyc": "^15.1.0",
    "push-dir": "^0.4.1",
    "semver-extract": "^1.1.1"
  },
  "dependencies": {
    "@koumoul/nuxt-config-inject": "^0.4.10",
    "@koumoul/sd-express": "1.0.0",
    "@koumoul/sd-vue": "1.2.0",
    "@koumoul/vue-multianalytics": "^1.10.1",
    "@mdi/font": "^5.1.45",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "accept-language-parser": "^1.5.0",
    "axios": "^0.21.1",
    "babel-plugin-transform-imports": "^1.5.1",
    "body-parser": "^1.18.3",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.3",
    "cookie-universal-nuxt": "^2.1.5",
    "cookies": "^0.7.3",
    "dayjs": "^1.10.7",
    "debug": "^3.2.6",
    "email-validator": "^2.0.4",
    "event-to-promise": "^0.8.0",
    "express": "^4.17.1",
    "flat": "^4.1.0",
    "fs-extra": "^8.1.0",
    "iframe-resizer": "^4.0.1",
    "json-web-key": "^0.3.0",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "jwt-decode": "^2.2.0",
    "ldapjs": "^2.2.3",
    "maildev": "^1.1.0",
    "marked": "^0.3.17",
    "material-colors": "^1.2.6",
    "material-design-icons-iconfont": "^5.0.1",
    "memoizee": "^0.4.15",
    "mjml": "^4.8.1",
    "mongodb": "^3.6.4",
    "multer": "^1.4.2",
    "no-avatar": "^1.0.1",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.18.4",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.27.0",
    "original-url": "^1.2.3",
    "otplib": "^12.0.1",
